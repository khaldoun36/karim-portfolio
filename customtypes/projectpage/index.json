{
  "format": "page",
  "id": "projectpage",
  "label": "ProjectPage",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "subtitle": {
        "type": "Text",
        "config": {
          "label": "Subtitle",
          "placeholder": ""
        }
      },
      "main_image": {
        "type": "Image",
        "config": {
          "label": "Main Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "category": {
        "type": "Select",
        "config": {
          "label": "Category",
          "placeholder": "",
          "options": ["Digital Marketing", "SEO Optimization"],
          "default_value": "Digital Marketing"
        }
      },
      "important_metrics": {
        "type": "Group",
        "config": {
          "label": "Important Metrics",
          "repeat": true,
          "fields": {
            "description": {
              "type": "Text",
              "config": {
                "label": "Description",
                "placeholder": ""
              }
            },
            "percentage": {
              "type": "Number",
              "config": {
                "label": "Percentage",
                "placeholder": ""
              }
            }
          }
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "call_to_action": {
              "type": "SharedSlice"
            },
            "rich_text": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
